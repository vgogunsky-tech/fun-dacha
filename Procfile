web: bash -lc "git config --global --add safe.directory $(pwd) && (git remote set-url origin https://x-access-token:${GIT_AUTH_TOKEN}@github.com/volodymyr-hohunskyi/fun-dacha.git || git remote add origin https://x-access-token:${GIT_AUTH_TOKEN}@github.com/volodymyr-hohunskyi/fun-dacha.git) && python webapp/app.py"

